<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<!-- 指定工作空间，要与接口名相同，源代码没有去看，猜测应该是通过"这里的namespace.下边方法的id"来定位方法的 -->
<mapper namespace="com.example.demo.Mapper.StudentMapper">
    <resultMap id="studentmap" type="com.example.demo.Entity.TeamStudentEntity">
        <id column="id" property="id"/>
        <result column="student_id" property="studentId"/>
        <result column="team_id" property="teamId"/>
        <result column="course_id" property="courseId"/>
    </resultMap>

    <select id="findTeamId" resultType="com.example.demo.Entity.TeamStudentEntity" resultMap="studentmap">
    select * from team_student where student_id= #{studentId}
</select>

    <insert id="ask">
        insert into question(student_id, seminar_id, team_id, present_id, round) value (#{arg0},#{arg1},#{arg2},#{arg3},#{arg4})
    </insert>

</mapper>